<% workflows.each do |workflow| %>
  <a href="<%= workflow_analyses_path(workflow.dxid) %>"
    class="list-group-item word-break-all workflow-<%= workflow.workflow_series_id %> <%= workflow.workflow_series_id == active ? 'active' : ''%>"
    data-app="<%= workflow.dxid %>">
    <div class="app-list-group-item-title"><%= workflow.title %></div>
    <small class="text-muted"><%= workflow.user.org.handle %>/<%= workflow.user.dxuser %></small>
  </a>
<% end %>
